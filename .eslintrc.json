{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true,
        "amd": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
    "react/no-unescaped-entities": 0 
    },
    "parser":"babel-eslint",

  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
